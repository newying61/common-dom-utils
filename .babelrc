{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    [
        "@babel/plugin-transform-runtime",
        {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": true
        }
    ]
  ],
}
